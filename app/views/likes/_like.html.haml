- if user_signed_in?
  -if @prototype.like_user(current_user.id)
    = link_to prototype_like_path(@prototype.likes_count, prototype_id: @prototype.id), method: :delete, id: "like-button", remote: true do
      = image_tag("icon_red_heart.svg")
      %span.num
        = "LIKE #{@prototype.likes_count}"
  -else
    = link_to prototype_likes_path(@prototype), id: "like-button",method: :post, remote: true do
      =image_tag("icon_heart.svg")
      %span.num
        = "LIKE #{@prototype.likes_count}"
-else
  = image_tag("icon_heart.svg")
